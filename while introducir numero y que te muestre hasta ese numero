using System;

namespace Progracion
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Dime un numero");
            int num1 = int.Parse(Console.ReadLine());
            int contador = 1;
            while (contador <= num1)
            {

                Console.WriteLine(contador);
                contador = contador + 1;


            }
           
            Console.WriteLine("fin");
        }
    }
}
